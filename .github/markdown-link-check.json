{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^mailto:"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^../../issues",
      "replacement": "https://github.com/user/BuildFabricModServer/issues"
    },
    {
      "pattern": "^../../discussions",
      "replacement": "https://github.com/user/BuildFabricModServer/discussions"
    },
    {
      "pattern": "^../../pull",
      "replacement": "https://github.com/user/BuildFabricModServer/pull"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com", "https://api.github.com"],
      "headers": {
        "User-Agent": "markdown-link-check"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 304, 307, 308]
}